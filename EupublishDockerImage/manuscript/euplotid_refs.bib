  file = {Full Text PDF:/Users/dborgesr/Library/Application Support/Zotero/Profiles/1ypdwflt.default/zotero/storage/7VRQXWTJ/Geyer and Corces - 1992 - DNA position-specific repression of transcription .pdf:application/pdf;Snapshot:/Users/dborgesr/Library/Application Support/Zotero/Profiles/1ypdwflt.default/zotero/storage/5G66UP9S/1865.html:text/html},
