@article{dai_igf2bp2/imp2_2015,
  title = {{{IGF2BP2}}/{{IMP2 Deficient Mice Resist Obesity}} through Enhanced Translation of {{Ucp1 mRNA}} and Other {{mRNAs}} Encoding {{Mitochondrial Proteins}}},
