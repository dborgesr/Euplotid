@article{katainen_ctcf/cohesin-binding_2015,
  title = {{{CTCF}}/Cohesin-Binding Sites Are Frequently Mutated in Cancer},
  volume = {47},
  copyright = {\textcopyright{} 2015 Nature Publishing Group, a division of Macmillan Publishers Limited. All Rights Reserved.},
  issn = {1061-4036},
  doi = {10.1038/ng.3335},
  abstract = {Cohesin is present in almost all active enhancer regions, where it is associated with transcription factors. Cohesin frequently colocalizes with CTCF (CCCTC-binding factor), affecting genomic stability, expression and epigenetic homeostasis. Cohesin subunits are mutated in cancer, but CTCF/cohesin-binding sites (CBSs) in DNA have not been examined for mutations. Here we report frequent mutations at CBSs in cancers displaying a mutational signature where mutations in A\textbullet{}T base pairs predominate. Integration of whole-genome sequencing data from 213 colorectal cancer (CRC) samples and chromatin immunoprecipitation sequencing (ChIP-exo) data identified frequent point mutations at CBSs. In contrast, CRCs showing an ultramutator phenotype caused by defects in the exonuclease domain of DNA polymerase $\epsilon$ (POLE) displayed significantly fewer mutations at and adjacent to CBSs. Analysis of public data showed that multiple cancer types accumulate CBS mutations. CBSs are a major mutational hotspot in the noncoding cancer genome.},
  language = {en},
  timestamp = {2017-07-24T04:45:58Z},
  number = {7},
  urldate = {2017-07-24},
  journal = {Nature Genetics},
  author = {Katainen, Riku and Dave, Kashyap and Pitk{\"a}nen, Esa and Palin, Kimmo and Kivioja, Teemu and V{\"a}lim{\"a}ki, Niko and Gylfe, Alexandra E. and Ristolainen, Heikki and H{\"a}nninen, Ulrika A. and Cajuso, Tatiana and Kondelin, Johanna and Tanskanen, Tomas and Mecklin, Jukka-Pekka and J{\"a}rvinen, Heikki and Renkonen-Sinisalo, Laura and Lepist{\"o}, Anna and Kaasinen, Eevi and Kilpivaara, Outi and Tuupanen, Sari and Enge, Martin and Taipale, Jussi and Aaltonen, Lauri A.},
  month = jul,
