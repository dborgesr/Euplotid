  number = {2},
  urldate = {2017-09-11},
  journal = {Acta Naturae},
  author = {Fedotova, A. A. and Bonchuk, A. N. and Mogila, V. A. and Georgiev, P. G.},
  year = {2017},
  pages = {47--58},
  file = {PubMed Central Full Text PDF:/Users/dborgesr/Library/Application Support/Zotero/Profiles/1ypdwflt.default/zotero/storage/52UDHE2X/Fedotova et al. - 2017 - C2H2 Zinc Finger Proteins The Largest but Poorly .pdf:application/pdf},
  pmid = {28740726},
  pmcid = {PMC5509000}
}

