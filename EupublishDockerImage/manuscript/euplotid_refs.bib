  keywords = {Adolescent,Adult,Body Mass Index,Diabetes Mellitus; Type 2,Female,Follow-Up Studies,Guidelines as Topic,Humans,Middle Aged,Prospective Studies,Risk Factors,Surveys and Questionnaires,United States,United States Department of Agriculture,Weight Gain},
  pages = {481--486},
  groups = {igf2bp2},
