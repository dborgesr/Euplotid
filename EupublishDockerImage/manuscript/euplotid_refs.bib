  month = mar,
  year = {2009},
  keywords = {bioethanol,biomass conversion,Cellulase,cell wall hydrolysis,high throughput screening,plant pathogens},
  pages = {1033--1044},
  file = {Full Text PDF:/Users/dborgesr/Library/Application Support/Zotero/Profiles/1ypdwflt.default/zotero/storage/R5F8R7CK/King et al. - 2009 - An optimized microplate assay system for quantitat.pdf:application/pdf;Snapshot:/Users/dborgesr/Library/Application Support/Zotero/Profiles/1ypdwflt.default/zotero/storage/CUZ4Q5RF/abstract.html:text/html},
