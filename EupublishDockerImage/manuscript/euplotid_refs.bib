  file = {PubMed Central Full Text PDF:/Users/dborgesr/Library/Application Support/Zotero/Profiles/1ypdwflt.default/zotero/storage/GN44JGH3/Dai et al. - 2013 - mTOR complex 2 phosphorylates IMP1 cotranslational.pdf:application/pdf},
  groups = {igf2bp2},
