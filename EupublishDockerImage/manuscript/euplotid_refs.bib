  month = jan,
  year = {2012},
  pages = {1680--1688},
  file = {Full Text PDF:/Users/dborgesr/Library/Application Support/Zotero/Profiles/1ypdwflt.default/zotero/storage/2924J98F/Wang et al. - 2012 - Widespread plasticity in CTCF occupancy linked to .pdf:application/pdf;Snapshot:/Users/dborgesr/Library/Application Support/Zotero/Profiles/1ypdwflt.default/zotero/storage/9ZVEXDPS/1680.html:text/html},
