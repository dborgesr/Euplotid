  keywords = {2;4-Dinitrophenol,Animals,Blood Glucose,Cell Respiration,Energy Metabolism,Female,Insulin,Longevity,Mice,mitochondria,Oxidation-Reduction,oxidative stress,Triglycerides,Uncoupling Agents},
  pages = {552--560},
