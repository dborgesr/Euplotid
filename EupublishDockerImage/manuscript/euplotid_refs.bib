  file = {PubMed Central Full Text PDF:/Users/dborgesr/Library/Application Support/Zotero/Profiles/1ypdwflt.default/zotero/storage/FV7PEAN5/Janiszewska et al. - 2012 - Imp2 controls oxidative phosphorylation and is cru.pdf:application/pdf},
