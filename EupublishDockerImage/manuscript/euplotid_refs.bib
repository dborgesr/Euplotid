  keywords = {Aging,Animal Feed,Animals,Body Weight,Diet,Disease Susceptibility,Edible Grain,Female,Food Deprivation,Longevity,Male,Mice,Mice; Inbred C57BL,Survival Rate},
  pages = {B379--389},
  groups = {igf2bp2},
