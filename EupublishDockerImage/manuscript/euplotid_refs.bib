  pages = {185--197},
  file = {ScienceDirect Snapshot:/Users/dborgesr/Library/Application Support/Zotero/Profiles/1ypdwflt.default/zotero/storage/K9MHXXUT/0377840194901716.html:text/html},
  groups = {gut\_txome}
}

