  month = dec,
  year = {2013},
  keywords = {chromosome organization,Cohesin,transcription},
  pages = {3119--3129},
  file = {Full Text PDF:/Users/dborgesr/Library/Application Support/Zotero/Profiles/1ypdwflt.default/zotero/storage/JHK4MBKA/Sofueva et al. - 2013 - Cohesin‐mediated interactions organize chromosomal.pdf:application/pdf;Snapshot:/Users/dborgesr/Library/Application Support/Zotero/Profiles/1ypdwflt.default/zotero/storage/E7RQAR48/3119.html:text/html},
