  month = jul,
  year = {2011},
  keywords = {epigenetics,Functional genomics,Pluripotent stem cells},
  pages = {630--638},
  file = {Snapshot:/Users/dborgesr/Library/Application Support/Zotero/Profiles/1ypdwflt.default/zotero/storage/VH9D8W9S/ng.857.html:text/html},
