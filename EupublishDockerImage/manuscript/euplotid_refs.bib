  month = jan,
  year = {2014},
  pages = {1905--1917},
  file = {Full Text PDF:/Users/dborgesr/Library/Application Support/Zotero/Profiles/1ypdwflt.default/zotero/storage/32XR9J8N/Heidari et al. - 2014 - Genome-wide map of regulatory interactions in the .pdf:application/pdf;Snapshot:/Users/dborgesr/Library/Application Support/Zotero/Profiles/1ypdwflt.default/zotero/storage/GHEFIMR9/1905.html:text/html},
