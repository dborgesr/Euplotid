  month = jun,
  year = {2008},
  keywords = {centromere,cohesion,insulator,metaphase,transcription},
  pages = {8309--8314},
  file = {Full Text PDF:/Users/dborgesr/Library/Application Support/Zotero/Profiles/1ypdwflt.default/zotero/storage/I7QDTXC9/Rubio et al. - 2008 - CTCF physically links cohesin to chromatin.pdf:application/pdf;Snapshot:/Users/dborgesr/Library/Application Support/Zotero/Profiles/1ypdwflt.default/zotero/storage/U7KJMD7Q/8309.html:text/html},
