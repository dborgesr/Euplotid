  pages = {2066--2077},
  file = {Full Text PDF:/Users/dborgesr/Library/Application Support/Zotero/Profiles/1ypdwflt.default/zotero/storage/GB9TPJ4M/Seitan et al. - 2013 - Cohesin-based chromatin interactions enable regula.pdf:application/pdf;Snapshot:/Users/dborgesr/Library/Application Support/Zotero/Profiles/1ypdwflt.default/zotero/storage/6IKEE53N/2066.html:text/html},
